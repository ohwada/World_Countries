<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2">
    <Document id="1">
        <name>Screen Overlay Demo</name>
        <description>display Legend: Wetlands at centor of screen</description>
        <ScreenOverlay id="2">
            <name>Legend: Wetlands</name>
            <Icon id="3">
                <href>https://raw.githubusercontent.com/ohwada/World_Countries/master//simplekml/screen_overlay/images/legend_wetlands.jpg</href>
            </Icon>
            <overlayXY x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
            <screenXY x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
            <size x="-1" y="-1" xunits="fraction" yunits="fraction"/>
        </ScreenOverlay>
    </Document>
</kml>
